CREATE TABLE impiegato (
  matricola int NOT NULL AUTO_INCREMENT,
  nome varchar(75) NOT NULL,
  cognome varchar(75) NOT NULL,
  email varchar(75) NOT NULL,
  passw varchar(75) NOT NULL,
  stipendio int,
  PRIMARY KEY (matricola)
);

DB: azienda
